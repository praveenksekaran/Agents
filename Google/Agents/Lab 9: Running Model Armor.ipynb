    "os.environ['user_prompt_uri'] = \"Can you describe this link? https://testsafebrowsing.appspot.com/s/malware.html\" # @param {\"type\":\"string\",\"placeholder\":\"Input a prompt you wish to screen for Malicious URI filtering\"}"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
